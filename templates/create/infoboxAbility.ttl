@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix ex: <http://localhost:8000/vocab#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .




# Class Definition for Ability
local:Ability a rdfs:Class ;
    rdfs:label "Ability"@en ;
    rdfs:comment "A class representing Pokémon abilities, which define special powers or characteristics in the Pokémon universe."@en ;
    rdfs:subClassOf schema:Thing .



local:introduced_generation a rdfs:Property ;
    rdfs:label "Introduced in Generation"@en ;
    rdfs:comment "The generation the ability was introduced in."@en ;
    rdfs:domain local:Ability ;
    rdfs:range xsd:integer .

