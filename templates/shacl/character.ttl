@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix local: <http://localhost:8000/vocab#> .

local:CharacterShape rdf:type sh:NodeShape ;
    sh:targetClass local:Character ;
    sh:property [
        sh:path local:color ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path local:borderColor ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path local:coreColor ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path local:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path local:jName ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path local:tmName ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path local:age ;
        sh:datatype xsd:integer ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path local:birthday ;
        sh:datatype xsd:date ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path local:gender ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path local:height ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path local:eyes ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path local:hair ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path local:hometown ;
        sh:class local:location ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path local:Region ;
        sh:class local:Region ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path local:relatives ;
        sh:class rdf:List ;
    ] .
